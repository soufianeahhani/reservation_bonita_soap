<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from GestionDeReservationService WSDL service -->

  <service name="GestionDeReservation" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://services"/>
      <parameter name="wsdlServiceElement" value="GestionDeReservationService"/>
      <parameter name="schemaQualified" value="http://services"/>
      <parameter name="wsdlServicePort" value="GestionDeReservation"/>
      <parameter name="className" value="services.GestionDeReservationSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="GestionDeReservation"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="etudeEtatDeReservationParTypeDePaiement" qname="operNS:etudeEtatDeReservationParTypeDePaiement" xmlns:operNS="http://services" soapAction="" >
        <parameter qname="pns:type_paiement" xmlns:pns="http://services" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:id_voyage" xmlns:pns="http://services" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:id_client" xmlns:pns="http://services" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="etudeEtatDeReservationParTypeDePaiement"/>

  </service>
</deployment>
